{
  "name": "sithira/baum",
  "type": "library",
  "version": "1.0",
  "description": "Baum is an implementation of the Nested Set pattern for Eloquent models.",
  "keywords": ["nested set", "laravel", "laravel 5", "eloquent", "database"],
  "license": "MIT",
  "authors": [
    {
      "name": "Estanislau Trepat",
