      "homepage": "http://etrepat.com"
    },
    {
      "name": "Gary Pearman",
      "email": "gaz@red-robot.co.uk",
      "homepage": "http://red-robot.co.uk"
    },
    {
      "name": "Martin Seysen",
      "email": "mse@dogado.de",
      "homepage": "https://www.dogado.de"
    },
    {
      "name": "Sithira Munasinghe",
      "email": "sithiraac@gmail.com",
      "homepage": "http://github.com/Sithira"
    }

  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/console": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|6.*.*",
    "illuminate/database": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|6.*.*",
    "illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|6.*.*",
    "illuminate/filesystem": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|6.*.*",
    "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|6.*.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "~0.9",
    "d11wtq/boris": "~1.0.10",
    "satooshi/php-coveralls": "dev-master",
    "fabpot/php-cs-fixer": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "Baum\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Baum\\Providers\\BaumServiceProvider"
      ]
    }
  }
